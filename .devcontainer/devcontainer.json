{
  "name": "TicketFlow",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspace",
  "forwardPorts": [
    8000,
    3306,
    7700
  ],
  "postCreateCommand": "composer install && npm ci",
  "remoteEnv": {
    "GEMINI_API_KEY": "${GEMINI_API_KEY}",
    "JITSI_VPAAS_MAGIC_COOKIE": "${JITSI_VPAAS_MAGIC_COOKIE}"
  }
}